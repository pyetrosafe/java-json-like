{
  "name": "java-object-formatter",
  "displayName": "Java Object Formatter",
  "description": "Formats Java object strings, similar to JSON.",
  "version": "0.0.1",
  "publisher": "YourName",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:java-object-formatter.formatSelectedText"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "java-object-formatter.formatSelectedText",
      "title": "Format Java Object String"
    }]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.60.0",
    "typescript": "^4.4.4"
  }
}